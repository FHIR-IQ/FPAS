{
  "name": "fhir-iq-prior-auth-poc",
  "version": "1.0.0",
  "description": "FHIR IQ Prior Authorization System - Proof of Concept",
  "main": "dist/src/app.js",
  "scripts": {
    "build": "tsc",
    "vercel-build": "npm run build",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "start": "node dist/src/app.js",
    "start:prod": "NODE_ENV=production node dist/src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:performance": "artillery run tests/performance/load-test-pas.yml",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "validate-fhir": "node scripts/validate-fhir.js",
    "inferno-tests": "bash scripts/run-inferno-tests.sh",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "seed-data": "node scripts/seed-test-data.js",
    "generate-docs": "swagger-jsdoc -d swaggerDef.js src/routes/*.ts -o docs/swagger.json",
    "demo:testdata": "node scripts/seed-test-data.js",
    "demo:submit": "node scripts/demo-submit.js",
    "demo:status": "node scripts/demo-status.js",
    "demo:dtr": "node scripts/demo-dtr.js",
    "demo:crd": "node scripts/demo-crd-hooks.js",
    "demo:vendor-adapters": "node scripts/demo-vendor-adapters.js",
    "demo:full-workflow": "node scripts/demo-full-workflow.js",
    "test:cms-compliance": "echo 'Running CMS API compliance tests...' && npm run test:pa-api && npm run test:fhir-validation",
    "test:pa-api": "echo 'Testing PA API compliance...' && npm run demo:submit && npm run demo:status",
    "test:fhir-validation": "node scripts/validate-fhir.js",
    "test:inferno-dtr": "bash scripts/inferno-dtr-setup.sh",
    "test:cql-prepopulation": "echo 'CQL prepopulation testing requires Inferno DTR kit - run npm run test:inferno-dtr'",
    "validate:ig-versions": "node -e \"console.log('Validating IG versions in CapabilityStatement...'); const fs = require('fs'); const cs = JSON.parse(fs.readFileSync('../specs/fhir/capabilitystatements/payer-pas-capabilitystatement.json')); const igs = cs.implementationGuide; console.log('IG versions:', igs); process.exit(igs.every(ig => ig.includes('|')) ? 0 : 1);\""
  },
  "dependencies": {
    "fastify": "^4.24.0",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@fastify/oauth2": "^7.5.0",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.0.1",
    "fhir": "^4.11.1",
    "@types/fhir": "^0.0.37",
    "bullmq": "^4.15.0",
    "ioredis": "^5.3.2",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "axios": "^1.6.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-client": "^3.0.1",
    "uuid": "^9.0.1",
    "date-fns": "^2.30.0",
    "winston": "^3.11.0",
    "pino": "^8.16.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/uuid": "^9.0.6",
    "@types/jsonwebtoken": "^9.0.4",
    "typescript": "^5.2.0",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.5",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.15",
    "eslint": "^8.52.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "prettier": "^3.0.3",
    "artillery": "^2.0.0",
    "swagger-jsdoc": "^6.2.8"
  },
  "keywords": [
    "fhir",
    "prior-authorization",
    "davinci-pas",
    "davinci-dtr",
    "healthcare",
    "interoperability",
    "cms-0057-f"
  ],
  "author": "FHIR IQ Development Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/fhir-iq-prior-auth.git"
  }
}